{
  "name":        "nubium/pillar",
  "description": "Yet another take on ORM. This time with both hookers AND blackjack.",
  "keywords":    ["orm", "model", "entity", "mysql", "database", "persistence"],
  "license":     "MIT",
  "require": {
    "php": ">=7.4",
    "dibi/dibi": "~4.2.5",
    "spareparts/enum": "^1.1.2",
    "doctrine/annotations": "^1.13"
  },
  "require-dev": {
    "ext-json": "*",
    "phpunit/phpunit": "^9.5",
    "mockery/mockery": "^1.4",
    "phpstan/phpstan": "^1.4.2",
    "phpstan/phpstan-phpunit": "^1.0.0",
    "phpstan/phpstan-mockery": "^1.0.0",
    "phpstan/extension-installer": "^1.1.0"
  },
  "autoload": {
    "psr-4": {
      "SpareParts\\Pillar\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SpareParts\\Pillar\\Test\\": "test"
    }
  }
}
